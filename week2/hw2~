#lang racket

(define (series a b n)
  (cond
; a + b is the next number
    [(= n 1) a]
    [(= n 2) b]
    [else (define (series-inside current a b)
            (if (= current n) (+ a b)
                (series-inside (+ 1 current) b (+ a b))
                )
            )]
    
    )
 )
 
(define (lucas n)
  (series 2 1 n)
)
(define (fibonacci n)
  (series 1 1 n)
 )